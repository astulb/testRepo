@{
    ViewBag.Title = "CreatePersonView";
}

<h2>Create Person </h2>

@using (Html.BeginForm("CreateSubmit", "People", FormMethod.Post))
{
    @Html.AntiForgeryToken()

    @Html.Partial("_PersonFormView")
    <button type="submit" class="btn btn-default">Submit</button>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}